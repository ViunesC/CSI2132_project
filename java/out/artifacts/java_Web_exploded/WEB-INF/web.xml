<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>customerHome</servlet-name>
        <jsp-file>/Home/Customer.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>customerHome</servlet-name>
        <url-pattern>/customer</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>employeeHome</servlet-name>
        <jsp-file>/Home/Employee.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>employeeHome</servlet-name>
        <url-pattern>/employee</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>adminHome</servlet-name>
        <jsp-file>/Home/Admin.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>adminHome</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>customerRes</servlet-name>
        <jsp-file>/Home/Customer_result.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>customerRes</servlet-name>
        <url-pattern>/customer_result</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>customerInfo</servlet-name>
        <jsp-file>/Home/Customer_info.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>customerInfo</servlet-name>
        <url-pattern>/customer_info</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>employeeInfo</servlet-name>
        <jsp-file>/Home/Employee_info.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>employeeInfo</servlet-name>
        <url-pattern>/employee_info</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>employeeRes</servlet-name>
        <jsp-file>/Home/Employee_result.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>employeeRes</servlet-name>
        <url-pattern>/employee_result</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>employeeRent</servlet-name>
        <jsp-file>/Home/Employee_createRenting.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>employeeRent</servlet-name>
        <url-pattern>/employee_rent</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>employeeRentRes</servlet-name>
        <jsp-file>/Home/Employee_rent_result.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>employeeRentRes</servlet-name>
        <url-pattern>/employee_rent_result</url-pattern>
    </servlet-mapping>
</web-app>